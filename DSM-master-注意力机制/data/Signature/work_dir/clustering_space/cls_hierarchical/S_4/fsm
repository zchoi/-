digraph Automata {
rankdir=LR
size="8,5"
	node [shape=star]
	CSTART
	node [shape=doublecircle]
	CEND
	node [shape=circle]
	C2 -> C0 [label=initVerify]
	C2 -> C2 [label="verify:FALSE"]
	C0 -> CEND [label=<END>]
	C0 -> C1 [label=update]
	C0 -> C2 [label="verify:FALSE"]
	C2 -> C1 [label=update]
	C1 -> C0 [label=initVerify]
	C2 -> CEND [label=<END>]
	C1 -> C2 [label="verify:FALSE"]
	C1 -> CEND [label=<END>]
	C0 -> C0 [label=initVerify]
	CSTART -> C0 [label=Signature]
	C1 -> C1 [label=update]
}
