digraph Automata {
rankdir=LR
size="8,5"
	node [shape=star]
	CSTART
	node [shape=doublecircle]
	CEND
	node [shape=circle]
	C4 -> CEND [label=<END>]
	C4 -> C2 [label=initVerify]
	C3 -> C3 [label=update]
	C0 -> CEND [label=<END>]
	CSTART -> C4 [label=Signature]
	C0 -> C0 [label="verify:FALSE"]
	C2 -> C0 [label="verify:FALSE"]
	C2 -> C2 [label=initVerify]
	C2 -> CEND [label=<END>]
	C2 -> C3 [label=update]
	C3 -> C2 [label=initVerify]
	C0 -> C2 [label=initVerify]
	C0 -> C3 [label=update]
	C3 -> C0 [label="verify:FALSE"]
	C3 -> CEND [label=<END>]
}
