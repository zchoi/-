digraph Automata {
rankdir=LR
size="8,5"
	node [shape=star]
	CSTART
	node [shape=doublecircle]
	CEND
	node [shape=circle]
	C1 -> CEND [label=<END>]
	C1 -> C1 [label=close]
	C0 -> C1 [label=close]
	C2 -> C2 [label=putNextEntry]
	C0 -> C2 [label=putNextEntry]
	C2 -> C0 [label=closeEntry]
	C2 -> C1 [label=close]
	C2 -> C2 [label=write]
	C0 -> C0 [label=write]
	C0 -> C0 [label=closeEntry]
	CSTART -> C0 [label=ZipOutputStream]
}
