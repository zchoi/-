digraph Automata {
rankdir=LR
size="8,5"
	node [shape=star]
	CSTART
	node [shape=doublecircle]
	CEND
	node [shape=circle]
	C1 -> C0 [label=close]
	C2 -> C1 [label=putNextEntry]
	C3 -> C1 [label=putNextEntry]
	C2 -> C0 [label=close]
	C1 -> C1 [label=putNextEntry]
	C3 -> C0 [label=close]
	C3 -> C3 [label=write]
	C0 -> C0 [label=close]
	CSTART -> C2 [label=ZipOutputStream]
	C2 -> C3 [label=write]
	C2 -> C2 [label=closeEntry]
	C1 -> C3 [label=write]
	C3 -> C2 [label=closeEntry]
	C1 -> C2 [label=closeEntry]
	C0 -> CEND [label=<END>]
}
